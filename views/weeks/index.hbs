<div id='site-wrapper'>
    <header>
        <div class='semicircle-top'>
            <p class='site-title'>pantry</p>
        </div>
    </header>
    <main>
        <div class='cushion'></div>
        <div class='whatkindofweekisit'> What Kind of </div>
        <div class='whatkindofweekisit'>Week Is It?</div>
        <div id='weeks-grid'>
            {{#each week}}
            <div>
                <a href='/weeks/{{this._id}}'>
                    <img class='thumbnail' src='{{this.img}}' alt='{{this.name}}'>
                    <div>{{this.name}}</div>
                </a>

            </div>
            {{/each}}
        </div>
    </main>
    <footer>
        <div id='my-button'>
           <p><a href='/weeks/new'></a>Create A New Week!</a></p>
        </div>
    </footer>
</div>